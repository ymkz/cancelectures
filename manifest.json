{
  "manifest_version": 2,
  "name": "Cancelectures",
  "description": "島根大学の休講情報を表示する拡張機能",
  "version": "1.0.2",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
        "19": "icons/icon19.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://www.kougi.shimane-u.ac.jp/*"
  ]
}
